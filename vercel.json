{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/admin/login/login.html"
    },
    {
      "src": "/dashboard",
      "dest": "/frontend/admin/dashboard.html"
    },
    {
      "src": "/edit-artikel",
      "dest": "/frontend/admin/edit_artikel.html"
    },
    {
      "src": "/form-buat",
      "dest": "/frontend/admin/form_buat.html"
    },
    {
      "src": "/login",
      "dest": "/frontend/admin/login/login.html"
    },
    {
      "src": "/register",
      "dest": "/frontend/admin/login/register.html"
    },
    {
      "src": "/preview",
      "dest": "/frontend/admin/artikel.html"
    },
    {
      "src": "/upload-foto",
      "dest": "/frontend/admin/upload_foto.html"
    },
    {
      "src": "/upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
      }
    }
  ]
}